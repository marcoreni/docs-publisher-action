{
  "hooks": {
    "after:init": ["pnpm run all"]
  }, 
  "git": {
    "commitMessage": "v${version}",
    "requireCommits": true
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": false
  },
  "plugins": {
    "./scripts/release-it/moving-git-tag-plugin.mjs": {
      "key": "value"
    }
  }
}